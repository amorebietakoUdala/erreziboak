{% extends 'base.html.twig' %}

{% block title %}Upload File
{% endblock %}

{% block message %}
	{% for msg in app.session.flashBag.get('success') %}
		<div class="alert alert-success margin-top-13">
			{{ msg | trans}}
		</div>
	{% endfor %}
	{% for msg in app.session.flashBag.get('error') %}
		<div class="alert alert-danger margin-top-13">
			{{ msg }}
		</div>
	{% endfor %}
	{% for msg in app.session.flashBag.get('warning') %}
		<div class="alert alert-warning margin-top-13">
			{{ msg | trans | capitalize }}
		</div>
	{% endfor %}
{% endblock message %}


{% block body %}
	<main class="container-fluid">
		{{ include ('receipts_file/_form.html.twig') }}
	</main>
{% endblock %}

{% block stylesheets %}
	{{ parent() }}
	{{ encore_entry_link_tags('receiptsFile_upload') }}
{% endblock %}

{% block javascripts %}
	{{ parent() }}
	{{ encore_entry_script_tags('receiptsFile_upload') }}
{% endblock %}
